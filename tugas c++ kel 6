#include<iostream>
#include <conio.h>
#include <math.h>
using namespace std;

main()
(
    int a=45, b=4, c=2;
    float ipangkat, iakar, isin, icos, itan;

    iakar = sqrt(b);
    cout<<"Akar dari "<<b<<" adalah"<<ipangkat<<end1;

    ipangkat = pow(b,c);
    cout<<b<," pangkat "<<c<<" adalah"<<ipangkat<<end1;

    isin =sin(a);
    cout<<"sin sudut "<<a<<" derajat adalah"<<isin<<end1;

    icos = cos(a);
    cout<<"cos sudut "<<a<<" derajat adalah"<<<icos<<end1;

    itan + tan(a) ;
    cout<<"tan sudut "<<a<<" derajat adalah"<<itan;

    getch();
}
